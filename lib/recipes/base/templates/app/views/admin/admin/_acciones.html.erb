<%= botones[:mover] %>
<div class="btn-group dropdown">
  <%= boton_dropdown %>
  <ul class="dropdown-menu pull-right" role="menu" aria-labelledby="dLabel">
    <%= content_tag :li, botones[:editar] if botones[:editar] %>
    <%= content_tag :li, botones[:ver] if botones[:ver] %>
    <% for boton in botones[:nested].to_a %>
      <%= content_tag :li, boton %>
    <% end %>
    <%= content_tag :li, botones[:archivos] if botones[:archivos] %>
    <li class="divider"></li>
    <%= content_tag :li, botones[:eliminar] if botones[:eliminar] %>
  </ul>
</div>