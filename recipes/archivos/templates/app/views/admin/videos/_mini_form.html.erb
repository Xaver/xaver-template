<%= mini_form_for propietario, :as => :propietario, :url => [:crear, :admin, propietario, :archivos], :method => :post do |f| %>
  <%= f.simple_fields_for :videos_attributes, :index => nil do |foto| %>
    <%= foto.input :titulo, :placeholder => 'Escriba el título...' %>
    <%= foto.input :codigo, :placeholder => 'Escriba el código de youtube...' %>
  <% end %>
  <%= hidden_field_tag :anchor, :videos %>
  <%= f.button :submit, :name => nil, :data => { :disable_with => t('espere') }, :class => 'btn btn-primary' %>
<% end %>