<%= mini_form_for propietario, :url => url do |f| %>
  <%= hidden_field_tag :url, admin_polymorphic_path(propietario, :fotos) %>
  <%= render "campos", :f => f %>
  <script type="text/javascript">
    $(".agregar-foto").click(function(){
      $("#mini div:last").after('<%= j render("campos", :f => f) %>');
    });
  </script>
  <%= f.button :submit, :name => nil, :disable_with => t("espere"), :class => 'btn btn-primary' %>
<% end %>
